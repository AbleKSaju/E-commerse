<body>
   
    <div class="screen-overlay"></div>

        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header>
              
                
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <!-- ... your table content ... -->
                        </table>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Product</th>
                                    <th>Name</th>
                                    <th>Quality</th>
                                    <th>Size</th>
                                    <th>Total</th>
                                    <th>Status</th>
                                    <th>Date</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                
                                <%for(i=order.length-1;i>=0;i--){%>
                               <tr>
                                <a href="<%=order[i].orderDetails[0]._id%>"> <td> <img style="height:60px ;width: 60px;" src="/uploads/products/<%=order[i].orderDetails[0].image[0]%>" alt=""> </td>
                                    <td><b class="fs-5"><%=order[i].orderDetails[0].name%></b></td>
                                    <td><b><%=order[i].quantity%></b></td>
                                    <td><b><%=order[i].size%></b></td>
                                  
                                    <!-- <td><%= order[i].orderDetails[0].quantity * order[i].orderDetails[0].saleprice %></td> -->
                                    <td>â‚¹ <%=order[i].quantity * parseInt(order[i].orderDetails[0].saleprice)%></td>
                                    <td>
                                        </a>
                                    <%if(order[i].status==0 || order[i].status==1){%>
                                        <span class="badge rounded-pill alert-warning">Pending</span>
                                          <%}else if(order[i].status==-1){%>
                                            <span class="badge rounded-pill alert-danger">cancelled </span>
                                            <%}else if(order[i].status==2){%>
                                                    <span class="badge rounded-pill alert-success">Delivered</span>
                                                    <%}%>
                                </td>
                                    <td><%=order[i].date%></td>    
                                    <td>
                                      <button onclick="showDetails('<%=data.name%>','<%=data.mobile%>','<%=order[i].address[0].altNumber%>','<%=order[i].address[0].pinCode%>','<%=order[i].address[0].house%>','<%=order[i].address[0].area%>','<%=order[i].address[0].landmark%>','<%=order[i].address[0].town%>','<%=order[i].address[0].state%>','<%=order[i].totalPrice%>','<%=order[i].status%>','<%=order[i].payment%>','<%=order[i].date%>')" class="btn btn-success">Details</button>
                                        <%if(order[i].status==0 || order[i].status==1){%>
                                        <button onclick="cancel('<%=order[i]._id%>')" class="btn btn-outline-danger">Cancel</button>
                                        <%}else if(order[i].status==-1){%>
                                            <button  class="btn btn-danger" disabled>cancelled</button>
                                            <%}else{%>
                                                <button class="btn btn-success muted-text" disabled>Delivered</button>
                                            <%}%>
                                    </td>
                                </tr>
                            <%}%>
                            </tbody>
                        </table>
                    </div>
                </div> 
            </div> 
         <img src="/public/images/delivered.jpeg" alt="">
        </section>
