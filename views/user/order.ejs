    <link href="/assets/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="screen-overlay"></div>

        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Product</th>
                                    <th>Name</th>
                                    <th>Quality</th>
                                    <th>Size</th>
                                    <th>Total</th>
                                    <th>Status</th>
                                    <th>Date</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(i=order.length-1;i>=0;i--){%>
                                <tr>
                                    <td> <img style="height:60px ;width: 60px;" src="/uploads/products/<%=order[i].orderDetails[0].image[0]%>" alt=""> </td>
                                    <td><b class="fs-5"><%=order[i].orderDetails[0].name%></b></td>
                                    <td><b><%=order[i].quantity%></b></td>
                                    <td><b><%=order[i].size%></b></td>
                                  
                                    <!-- <td><%= order[i].orderDetails[0].quantity * order[i].orderDetails[0].saleprice %></td> -->
                                    <td>â‚¹ <%=order[i].quantity * parseInt(order[i].orderDetails[0].saleprice)%></td>
                                    <td>
                                    <%if(order[i].status==0 || order[i].status==1){%>
                                        <span class="badge rounded-pill alert-warning">Pending</span>
                                          <%}else if(order[i].status==-1){%>
                                            <span class="badge rounded-pill alert-danger">cancelled </span>
                                            <%}else if(order[i].status==2){%>
                                                    <span class="badge rounded-pill alert-success">Delivered</span>
                                                    <%}%>
                                </td>
                                    <td><%=date%></td>
                                    <td>
                                        <a href="#" class="btn btn-md rounded font-sm">Detail</a>
                                    </td>
                                </tr>
                     <%}%>

                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
         
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <%-include('../../partials/user-footer')%>
        </footer>
    </main>
    <script src="assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assets/js/vendors/select2.min.js"></script>
    <script src="assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="assets/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="assets/js/main.js" type="text/javascript"></script>
</body>

<style>
    .table th, .table td {
    padding: 0.75rem;
    padding-right: 76px;
    vertical-align: top;
    border-top: 1px solid #dee2e6;
}
</style>
