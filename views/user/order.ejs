<script>

function showDetails(name, number, altNumber, pinCode, house, area, landmark, town, state, totalPrice, orderStatus, payment, createdOn) {
        if(orderStatus == '0' || orderStatus == '1') {
            imgUrl = "/images/pending.jpeg"
        }else if(orderStatus == '-1'){
            imgUrl = '/images/cancelled.jpeg'
        }else if(orderStatus == '2'){
            imgUrl = '/images/delivered.jpeg'
        }
        Swal.fire({
            title: '<strong><u>ORDER DETAILS</u></strong>',
            imageUrl: imgUrl,
            imageWidth: 400,
            imageHeight: 200,
            html:
                'Name : ' + name + '<br>' + ' Number: ' + number + ',' + ' Alternative: ' + altNumber + '<hr>' +
                '<h5>Delivery Address</h4> ' +
                '<span style="font-weight:bold">Pin code:</span>' + pinCode + ', &nbsp'+
                '<span style="font-weight:bold">House: </span>' + house + '<br>' +
                '<span style="font-weight:bold">Area: </span>' + area + ',' +
                '<span style="font-weight:bold">Landmark: </span>' + landmark + '<br>' +
                '<span style="font-weight:bold">Town: </span>' + town + ',' +
                '<span style="font-weight:bold">State: </span>' + state + '<br>' +
                '<h4>Other info</h4>' +
                '<span style="font-weight:bold">Order Status: </span>' + orderStatus + ','+
                '<span style="font-weight:bold"> Payment Method: </span>' + payment + '<br>' +
                '<span style="font-weight:bold">Grand Total: </span>' + totalPrice + '<br>' +
                '<span style="font-weight:bold">Ordered On: </span>' + createdOn + '<hr>',


            showCloseButton: true,
            focusConfirm: true,
            confirmButtonText:
                '<i class="fa fa-thumbs-up"></i> Conntinue!',
        })
    }

    function cancel(orderId){
        Swal.fire({
  title: 'Are you sure?',
  text: "You won't be able to revert this!",
  icon: 'warning',
  showCancelButton: true,
  confirmButtonColor: '#3085d6',
  cancelButtonColor: '#d33',
  confirmButtonText: 'Yes, Cancel Order!'
}).then((result) => {
  if (result.isConfirmed) {
        $.ajax({
            url:'/cancel',
            method:'post',
            data:{
                orderId
            },
            success:(response)=>{
                if(response.status=true){
          location.reload()
            }
            },
         
        })
  }
})
}
</script>


