    <script>
         function addtoCart(proId){
                                            var quantity=1
                                            var size=8
                                            $.ajax({
                                                 url:`/cart?id=${proId}`,
                                                 method:'post',
                                                 data:{
                                                    quantity:quantity,
                                                    size:size
                                                 },
                                                 success:(response)=>{
                                                    if (response.status === true) {
                                                        Swal.fire({
                                                          title: 'Added to Cart!',
                                                          text:'Continue Shopping',
                                                          icon: 'success',
                                                          timer: 3000, 
                                                          showConfirmButton: false 
                                                        })
                                                    }else{
                                                        alert("Error ocured")
                                                    }
                                                 }
                                            })
                                        }
                                        const anchorTags = document.getElementsByTagName("a");

// Loop through the anchor tags and remove the underline
for (let i = 0; i < anchorTags.length; i++) {
  anchorTags[i].style.textDecoration = "none";
}
    </script>
    
