            <body>
                <div class="colorlib-product">
                    <div class="container">
                        <div class="row">
                            <%-include('../../partials/products-aside')%>
                         
                            <div class="col-lg-9 col-xl-9">
                                <form class="input-group" action="/search">
                                    <input id="filterItem" type="text" name="search" class="form-control" placeholder="Search name">
                                    <button id="filterItems" class="btn btn-light bg mb-3" type="submit"> <i class="material-icons md-search"></i></button>
                                </form>
  
                                  <div class="row row-pb-md">
                                    <%if(products.length==0){%>
                                        <div style="display: flex; justify-content: center; align-items: center; height: 100vh;">
                                            <img src="/images/noproduct.png" alt="Product Not Found" style="height: 60%; width: 60%;">
                                          </div>
                                          
                                        <%}else{%>
                                    <% for (let i = 0; i < products.length; i++) { %>
                                      <div class="col-lg-4 mb-4 text-center">
                                        <div class="product-entry border">
                                          <a href="/product-display?id=<%= products[i]._id %>" class="img-wrap PROD-IMG">
                                            <img src="/uploads/products/<%= products[i].image[0] %>" style="height: 250px; width: 250px;" alt="Product">
                                          </a>
                                          <div class="desc">
                                            <h2 class="mb-0"><%= products[i].name %></h2>
                                            <div class="price"> 
                                              ₹ <%= products[i].saleprice %> &nbsp; &nbsp;
                                              <strike class="product_discount"> 
                                                <span style='color:black'>₹ <%= products[i].regularprice %></span>
                                              </strike>
                                            </div>
                                            <% if (isLoggedIn) { %>
                                              <b onclick="(addtoCart('<%= products[i]._id %>'))" class="btn btn-sm btn-brand rounded ml-5 mr-5">
                                                Add to Cart
                                              </b>
                                            <% } %>
                                            <div class="description mt-2">
                                              <% const words = products[i].description.split(' '); %>
                                              <% const truncatedDescription = words.slice(0, 7).join(' '); %>
                                              <%= truncatedDescription %>...
                                            </div>
                                          </div>
                                        </div>
                                      </div>
                                    <% } %>

                                  </div>
                                  <% if (totalPages) { %>
                                    <div class="pagination">
                                      <% if (currentPage > 1) { %>
                                        <button><a href="?page=<%= currentPage - 1 %>">Previous</a></button>
                                      <% } %>
                                  
                                      <% for (let i = 1; i <= totalPages; i++) { %>
                                        <button><a href="?page=<%= i %>" class="<%= i === currentPage ? 'active' : '' %>"><%= i %></a></button>
                                      <% } %>
                                  
                                      <% if (currentPage < totalPages) { %>
                                        <button><a href="?page=<%= currentPage + 1 %>">Next</a></button>
                                      <% } %>
                                    </div>
                                  <% } %>
                                  <%}%>
                            </div>
                        </div>
                    </div>
                </div>
            </body>     
