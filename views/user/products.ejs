            <aside id="colorlib-hero">
                <div class="flexslider">
                    <ul class="slides">
                        <% for(i = 0; i < banner.length; i++) { %>
                        <li class="slider-items" style="background-image: url(/uploads/banner/<%=banner[i].image%>)">
                            <div class="overlay"></div>
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-sm-6 offset-sm-3 text-center">
                                        <div class="slider-text-inner">
                                            <div class="desc">
                                                <h1 class="head-1 mt-5 text-white font-weight-bold animate-from-bottom-to-top"><%= banner[i].title %></h1>
                                                <h2 class="head-2 animate-from-bottom-to-top"></h2>
                                                <h2 class="head-3 text-white animate-from-bottom-to-top"><strong class="font-weight-bold">50%</strong> Off</h2>
                                                <% const descriptionWords = banner[i].description.split(' '); %>
                                                <% const truncatedDescription = descriptionWords.slice(0, 2).join(' '); %>
                                                <% const remainingDescription = descriptionWords.slice(2).join(' '); %>
                            
                                                <h2 class="category">
                                                    <span class="fs-1">
                                                        <h3 class="animate-from-bottom-to-top text-white"style="font-weight: normal;"><%= truncatedDescription %></h3>
                                                    </span>
                                                    <span class="fs-1" style="font-weight: normal;">
                                                        <h3 class="animate-from-bottom-to-top text-white"><%= remainingDescription %></h3>
                                                    </span>
                                                </h2>
                                                <p><a href="/brand/<%= banner[i].link %>" class="btn btn-primary animate-from-bottom-to-top">Shop Collection</a></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <% } %>
                    </ul>
                </div>
            </aside>
            
            <style>
                /* Define the animation */
                @keyframes animate-from-bottom-to-top {
                    0% {
                        transform: translateY(50%);
                        opacity: 0;
                    }
                    100% {
                        transform: translateY(0);
                        opacity: 1;
                    }
                }
            
                /* Apply animation to elements with the animate-from-bottom-to-top class */
                .animate-from-bottom-to-top {
                    animation: animate-from-bottom-to-top 1s ease-in-out;
                }
            </style>
            
            
            
