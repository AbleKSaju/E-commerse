<body>
    <div class="screen-overlay"></div>
   <%-include('../../partials/admin-header')%>
    <main class="main-wrap">
        
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products</h2>
                </div>
                <div>
                    <a href="/admin/admin-add-products" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                        </div>
                        <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                            <form class="input-group">
                                <input id="filterItem" type="text" name="search" class="form-control" placeholder="Search name">
                                <button id="filterItems" class="btn btn-light bg mb-3" type="submit"> <i class="material-icons md-search"></i></button>
                            </form>
                             
                            </select>
                        </div>

                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <article class="itemlist">
                      
                       
                        
                        <table id="grid" class="item row align-items-center table-spacing">
                            <tr>
                                <th>Product</th>
                                <th>description</th>
                                <th>Price</th>
                                <th>Status</th>
                                <th>Units</th>
                                <th>Action</th>
                            </tr>
                         
                            <% product.forEach(function(prod) { %>
                                <!-- <tr></tr> -->
                            <tr>
                                <td>
                                    <img src="/uploads/products/<%= prod.image[0] %>" class="img-sm img-thumbnail mt-3 mb-0" style="width: 130px;height: 80px;"  alt="Item">
            
                                    <h6 class="title mb-0"><%= prod.name %></h6>
                                </td>
                                <td>
                                   <p><%= prod.description %></p> 
                                </td>
                                <td>
                                    <%= prod.saleprice %>
                                </td>
                                <td>
                                    <%if(prod.verified==0){%>
                                    <span class="badge rounded-pill alert-success">Active</span>
                                    <%} else {%>
                                        <span class="badge rounded-pill alert-danger">Inactive</span>
                                        <%}%>
                                </td>
                                <td>
                                    <span><%= prod.units %></span>
                                </td>
                                <td class="col-action text-end ">
                                    <a href="/admin/edit-products/<%=prod._id%>" class="btn btn-sm font-sm rounded btn-brand">
                                        Edit
                                    </a>
                                    <!-- <a href="/admin/delete-product/<%= prod._id %>" class="btn btn-sm font-sm btn-light rounded">
                                        Del
                                    </a> -->
                                    <%if(prod.verified==0){%>
                                    <a href="/admin/unlist-product/<%= prod._id %>" class="btn btn-sm font-sm btn-outline-danger rounded">
                                        Unlist
                                    </a>
                                    <%} else {%>
                                    <a href="/admin/list-product/<%= prod._id %>" class="btn btn-sm font-sm btn-outline-danger rounded">
                                        List
                                    </a>
                                    <%}%>
                                </td>
                            </tr>
                            <% }) %>
                        </table>
                        <% if (totalPages) { %>
                            <div class="pagination">
                              <% if (currentPage > 1) { %>
                                <button><a href="?page=<%= currentPage - 1 %>">Previous</a></button>
                              <% } %>
                          
                              <% for (let i = 1; i <= totalPages; i++) { %>
                                <button><a href="?page=<%= i %>" class="<%= i === currentPage ? 'active' : '' %>"><%= i %></a></button>
                              <% } %>
                          
                              <% if (currentPage < totalPages) { %>
                                <button><a href="?page=<%= currentPage + 1 %>">Next</a></button>
                              <% } %>
                            </div>
                          <% } %>
                    </article> 
                   
