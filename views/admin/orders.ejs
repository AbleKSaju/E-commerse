    <script>
        // Initialize Bootstrap dropdown
        $(document).ready(function() {
            $('.dropdown-toggle').dropdown();
        });

        function cancelOrder(id){
            $.ajax({
    url: '/admin/cancelOrder',
    method: 'post',
    data: {
        id: id
    },
});

        }
        function makeOrder(id){
            $.ajax({
    url: '/admin/makeOrder',
    method: 'post',
    data: {
        id: id
    },
})

        }

        function approved(id){
            $.ajax({
    url: '/admin/approved',
    method: 'post',
    data: {
        id: id
    },
})

        }
    </script>
