    <main class="main-wrap">
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th scope="col">Name</th>
                                    <th scope="col">Number</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Method</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(i=0;i<data.length;i++){%>
                                <tr>
                                    <td><b><%=data[i].address[0].name%></b></td>
                                    <td><%=data[i].address[0].number%></td>
                                    <td><%=data[i].totalPrice%></td>
                                            <% if (data[i].status == 0) { %>
                                                <td><button class="badge rounded-pill alert-success text-bg-warning">Pending</button></td>
                                            <% } else if (data[i].status == -1) { %>
                                                <td><button class="badge rounded-pill alert-success text-bg-danger">Cancelled</button></td>
                                            <% } else if(data[i].status == 1){ %>
                                                <td><button class="badge rounded-pill alert-success text-bg-success">Approved</button></td>
                                            <% } else if(data[i].status == 2) {%>
                                                <td><button class="badge rounded-pill alert-success text-bg-success">Delivered</button></td>
                                                <%}%>
                                            
                                    <td><%=data[i].date%></td>
                                    <td><%=data[i].payment%></td>
                                    <td class="text-end">
                                                 <!-- <%if(data[i].status==1){%>
                                                    <a class="btn btn-success" href="">Delivered</a>
                                                    <%}%> -->
                                        <div class="dropdown pl-3">
                                            <a href="#" data-bs-toggle="dropdown" class=" mr-5 btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
                                            <div class="dropdown-menu">
                                                <%if(data[i].status==1){%>
                                                    <a class="dropdown-item text-danger" onclick="cancelOrder('<%=data[i]._id%>')" href="">Declined</a>
                                                    <%}%>
                                                <%if(data[i].status==0){%>
                                                <a class="dropdown-item text-danger" onclick="cancelOrder('<%=data[i]._id%>')" href="">Declined</a>
                                                <hr>
                                                <a class="dropdown-item text-success" onclick="approved('<%=data[i]._id%>')" href="">Approve</a>
                                                <%}else if(data[i].status==-1){%>
                                                    <a class="dropdown-item text-success" onclick="makeOrder('<%=data[i]._id%>')" href="">Re-oreder</a>
                                                       <%}%>
                            
                                                       <!-- <%if(data[i].status==2){%>
                                                        <a class="dropdown-item text-success" onclick="Delivered('<%=data[i]._id%>')" href="">Delivered</a>
                                                        <%}%> -->
                                            </div>
                                        </div>  
                                        
                                        <a href="/admin/details?id=<%=data[i]._id%>" class="btn btn-md rounded font-sm ml-5">Detail</a>
                                                                       
                                    </td>
     
                                </tr>
                                <%}%>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
            <div class="pagination-area mt-15 mb-50">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-start">
                        <li class="page-item active"><a class="page-link" href="#">01</a></li>
                        <li class="page-item"><a class="page-link" href="#">02</a></li>
                        <li class="page-item"><a class="page-link" href="#">03</a></li>
                        <li class="page-item"><a class="page-link dot" href="#">...</a></li>
                        <li class="page-item"><a class="page-link" href="#">16</a></li>
                        <li class="page-item"><a class="page-link" href="#"><i class="material-icons md-chevron_right"></i></a></li>
                    </ul>
                </nav>
            </div>
        </section> <!-- content-main end// -->
    </main><!-- Bootstrap CSS (include this in the head section) -->
